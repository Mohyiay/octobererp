                <SQL>
                    <QuerySql></QuerySql>
                    <SQLRaw>SELECT t.id as id, pro.name as project_name, CONCAT(u.first_name,' ', u.last_name) as supplier_name, t.check_in as atn_date , t.daily_wages as daily_wage , t.total_working_hour , t.overtime , t.check_in , t.check_out , t.total_wages, oe.name as employee_name, oe.employee_type as employee_type , t.supplier_id
                    FROM olabs_oims_attendances t
                    INNER JOIN olabs_oims_offrole_employees oe ON oe.id = t.employee_id AND oe.status = 1
                    INNER JOIN olabs_oims_projects pro ON t.project_id = pro.id
                    INNER JOIN backend_users u ON t.supplier_id =u.id
                    WHERE 1 = 1  
                    AND t.employee_type = 'offrole'
                    [ AND t.project_id in ( {projectsite} ) ]
                    [ AND t.supplier_id in ( {supplier} ) ]
                    [ AND  DATE(check_in)  {daterange} ]
                    [ AND oe.name LIKE "%{employee_filter,false}%" ]
                    order by check_in, t.supplier_id, oe.name</SQLRaw>
                </SQL>
