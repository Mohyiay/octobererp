                <CriteriaItem>
                    <Name>supplier</Name>
                    <Title>Supplier</Title>
                    <QueryTableName></QueryTableName>
                    <QueryColumnName></QueryColumnName>
                    <CriteriaType>LOOKUP</CriteriaType>
                    <CriteriaHelp>Start typing into the Supplier field. You will be presented with a list of  suppliers that match your query string.

                        Note that this is dynamically linked to the country criteria so that only suppliers from the selected countries are shown.</CriteriaHelp>
                    <CriteriaDisplay>DROPDOWN</CriteriaDisplay>
                    <ExpandDisplay>NOINPUT</ExpandDisplay>
                    <CriteriaRequired>.DEFAULT</CriteriaRequired>
                    <CriteriaHidden>.DEFAULT</CriteriaHidden>
                    <CriteriaDisplayGroup></CriteriaDisplayGroup>
                    <ReturnColumn>supplierId</ReturnColumn>
                    <DisplayColumn>supplierName</DisplayColumn>
                    <OverviewColumn>supplierName</OverviewColumn>
                    <MatchColumn>supplierName</MatchColumn>
                    <CriteriaDefaults></CriteriaDefaults>
                    <CriteriaList></CriteriaList>
                    <Query>
                        <TableSql></TableSql>
                        <WhereSql></WhereSql>
                        <GroupSql></GroupSql>
                        <RowSelection></RowSelection>
                        <SQLRaw>SELECT u.id as supplierId, concat( u.first_name,' ',u.last_name) as supplierName
                            FROM backend_users u
                            INNER JOIN backend_users_groups usg ON usg.user_id = u.id
                            INNER JOIN backend_user_groups ug ON usg.user_group_id = ug.id AND ug.code = 'inventory_supplier'
                            WHERE 1=1
                        </SQLRaw>
                        <SQL>
                            <QuerySql></QuerySql>
                        </SQL>
                        <QueryColumns>
                            <QueryColumn>
                                <Name>supplierId</Name>
                                <TableName></TableName>
                                <ColumnName>u.id as</ColumnName>
                                <ColumnType>char</ColumnType>
