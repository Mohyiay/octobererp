            <Assignments>
                <Assignment>
                    <AssignName>atn_date</AssignName>
                    <AssignNameNew></AssignNameNew>
                    <Expression>date(&quot;d/m/Y&quot;,strtotime({atn_date}))</Expression>
                    <Condition></Condition>
                </Assignment>
                <Assignment>
                    <AssignName>employee_type</AssignName>
                    <AssignNameNew></AssignNameNew>
                    <Expression>ucfirst({employee_type})</Expression>
                    <Condition></Condition>
                </Assignment>
                <Assignment>
                    <AssignName>check_in</AssignName>
                    <AssignNameNew></AssignNameNew>
                    <Expression>date(&quot;d/m/Y H:i&quot;,strtotime({check_in}))</Expression>
                    <Condition></Condition>
                </Assignment>
                <Assignment>
                    <AssignName>check_out</AssignName>
                    <AssignNameNew></AssignNameNew>
                    <Expression>date(&quot;d/m/Y H:i&quot;,strtotime({check_out}))</Expression>
                    <Condition></Condition>
                </Assignment>
                <Assignment>
                    <AssignName>grand_total</AssignName>
                    <AssignNameNew></AssignNameNew>
                    <Expression>sum({total_wages},{atn_date})</Expression>
                    <Condition></Condition>
                </Assignment>
                <Assignment>
                    <AssignName>grand_total</AssignName>
                    <AssignNameNew></AssignNameNew>
                    <Expression>&quot;&#8377;&quot;.number_format({grand_total},2)</Expression>
                    <Condition></Condition>
                </Assignment>
            </Assignments>
