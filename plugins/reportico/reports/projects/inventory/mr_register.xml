                <SQL>
                    <QuerySql></QuerySql>
                    <SQLRaw>SELECT t.id as id, pro.name as project_name, reference_number as mr_number, CONCAT(u.first_name,' ', u.last_name) as supplier_name, 
                        context_date as mr_date,t.total_price as grand_total, p.title as productname, pp.unit_price as price, pp.quantity, pp.total_price as  totalprice, pp.unit, t.status as mr_status
                        FROM olabs_oims_purchases t
                        INNER JOIN olabs_oims_purchase_products pp ON pp.purchase_id = t.id
                        INNER JOIN olabs_oims_products p ON pp.product_id = p.id
                        INNER JOIN olabs_oims_projects pro ON project_id = pro.id
                        INNER JOIN backend_users u ON user_id =u.id
                        WHERE 1 = 1  
                        [ AND t.project_id in ( {projectsite} ) ]
                        [ AND t.user_id in ( {supplier} ) ]
                        [ AND  DATE(context_date)  {daterange} ]
                        order by t.project_id, context_date, t.reference_number</SQLRaw>
                </SQL>
